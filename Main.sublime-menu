[
    {
        "id": "preferences",
        "children":
        [
            {
                "id": "package-settings",
                "children":
                [
                    {
                        "caption": "DocBlockr",
                        "children":
                        [
                            {
                                "caption": "Settings",
                                "command": "edit_settings", "args": {
                                    "base_file": "${packages}/DocBlockr/Base File.sublime-settings",
                                    "default": "{\n$0\n}\n"
                                }
                            },
                            { "caption": "-" },
                            {
                                "caption": "README",
                                "command": "open_file", "args":
                                {
                                    "file": "${packages}/DocBlockr/README.md"
                                }
                            },
                            {
                                "caption": "LICENSE",
                                "command": "open_file",
                                "args": {
                                    "file": "${packages}/DocBlockr/LICENSE"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]
